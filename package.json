    "@babel/preset-typescript": "^7.14.5",
    "@types/classnames": "^2.3.1",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.0.0",
