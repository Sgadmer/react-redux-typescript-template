    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/react-outside-click-handler": "^1.3.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/sass-loader": "^8.0.1",
