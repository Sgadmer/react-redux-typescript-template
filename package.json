{
  "name": "react-redux-typescript-template",
  "version": "1.0.0",
  "description": "React template for all basic needs",
  "private": "true",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode development",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack",
    "prettier": "prettier  --write src/**/*.js",
    "eslint": "eslint . --ext .js --ext .jsx --ext .d.ts --ext .tsx ",
    "eslint-fix": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix --color",
    "stylelint": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*.scss": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sgadmer/react-redux-typescript-template.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "webpack"
  ],
  "author": "Ruslan Khaziev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sgadmer/react-redux-typescript-template/issues"
  },
  "homepage": "https://github.com/Sgadmer/react-redux-typescript-template#readme",
